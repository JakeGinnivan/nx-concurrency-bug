{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/lib-1/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/lib-1",
        "commands": [
          "node ../../target.mjs lib1 build"
        ]
      }
    },
    "apply": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/lib-1",
        "commands": [
          "node ../../target.mjs lib1 apply"
        ],
        "forwardAllArgs": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/lib-1/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/lib-1"],
      "options": {
        "jestConfig": "packages/lib-1/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["lib-2"]
}
